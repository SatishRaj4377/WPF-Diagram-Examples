<Window x:Class="Selection_Override.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
        xmlns:local="clr-namespace:Selection_Override"
        Title="MainWindow" Height="350" Width="525">
    <Window.Resources>
        <Style TargetType="Path" x:Key="nodestyle">
            <Setter Property="Stretch" Value="Fill"></Setter>
            <Setter Property="Stroke" Value="Blue"></Setter>
            <Setter Property="Fill" Value="CornflowerBlue"></Setter>
        </Style>
    </Window.Resources>
    <Grid>
      <local:CustomDiagram x:Name="Diagram"></local:CustomDiagram>
    </Grid>
</Window>
